    android:layout_height="match_parent"
    tools:context="com.ist.cadillacpaltform.wifyapp.UI.Activity.QuestionActivity">

    <include layout="@layout/unit_title"></include>

    <RelativeLayout
        android:layout_below="@+id/rl_title"
        android:layout_width="match_parent"
