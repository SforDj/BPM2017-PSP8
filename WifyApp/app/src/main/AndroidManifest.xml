<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.ist.cadillacpaltform.wifyapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:replace="android:icon">
        <activity android:name=".UI.Activity.MainActivity"></activity>
        <activity android:name=".UI.Activity.SurveyActivity" />
        <activity android:name=".UI.Activity.LoginActivity" />
        <activity android:name=".UI.Activity.WelcomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".UI.Activity.TaskListActivity" />
        <activity android:name=".UI.Activity.QuestionActivity"></activity>
    </application>

</manifest>